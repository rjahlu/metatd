<script id="need_card" type="text/x-handlebars-template">
 <div class="horizontal_item wrapper needcard flex-col {{type}}"  data-router="/need/MyNeedItem/{{id}}">
    <div class="flex h100">
        <div class="item-thumb">
          <div class="image-thumbnail" style="background-image: url('{{image}}');">
            <span class="status hot">{{category}}</span>
            <a class="button button-raise btn-type btn-primary" href="/need/type/{{type}}/" style="margin-bottom: 8px;">{{type}}</a> 
          </div> 
        </div> 
        <div class="item-body">
          <p class="title">{{title}}</p>
          <p class="price">Giá: {{number price_from}} - {{number price_to}}</p>
          <p class="local">{{address}}</p>                      
        </div>
    </div>
    
</div>

</script>
 
</body>

